<tr id="project_<%= project.id%>">
    <td><%= project.name %></td>
    <td><%= project.team.name %></td>
    <td><%= project.description %></td>
   
    <td><%= project.initial_date %></td>
    <td><%= project.finish_date %></td>     
    <td><%= link_to 'Edit', edit_project_path(project) , remote: true%></td>
    <td><%= link_to 'Destroy', project, remote: true, method: :delete, data: { confirm: "Delete: #{project.name}?" } %></td>
</tr>