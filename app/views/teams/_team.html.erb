<tr id="team_<%= team.id%>">
    <td><%= team.name %></td>
    <td><%= team.color %></td>
    <td><%= team.users.count %></td>
    <td><ul><li type=”circle”><%= safe_join(team.users.map { |user| user.user_name }, ' - ') %></li></ul></td>
    <td class="text-center"><%= link_to edit_team_path(team), remote: true, class:"nav-item mx-2" do %>
            <i class="fas fa-edit icon"></i>
		<% end %></td>
    <td class="text-center"><%= link_to team, remote: true, method: :delete, data: { confirm: "Delete #{team.name}?"}, class:'nav-item mx-5'  do %>
            <i class="fas fa-trash-alt icon-trush"></i>
		<% end %></td>
</tr>